{
  "sourceRoot": "libs/thingsboard/thingsboard-interfaces/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/thingsboard/thingsboard-interfaces",
        "tsConfig": "libs/thingsboard/thingsboard-interfaces/tsconfig.lib.json",
        "packageJson": "libs/thingsboard/thingsboard-interfaces/package.json",
        "main": "libs/thingsboard/thingsboard-interfaces/src/index.ts",
        "assets": ["libs/thingsboard/thingsboard-interfaces/*.md"]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs thingsboard-thingsboard-interfaces {args.ver} {args.tag}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/thingsboard/thingsboard-interfaces/**/*.ts"]
      }
    }
  },
  "tags": []
}
